{
  "name": "front-door",
  "version": "0.0.1",
  "description": "Pennies front door for transactions",
  "main": "index.js",
  "repository": {},
  "license": "MIT",
  "scripts": {
    "prebuild": "rm -rf build build.zip",
    "build": "babel src --out-dir build --copy-files --ignore  **/*-test.js && cp package.json build && cd build && npm install --no-optional --production",
    "postbuild": "cd build && zip ../build.zip ./*",
    "test": "jasmine JASMINE_CONFIG_PATH=jasmine-config.json"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 4.3
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "standard": {
    "globals": [
      "describe",
      "it",
      "xit",
      "expect"
    ]
  },
  "dependencies": {
    "googleapis": "^17.1.0",
    "moment": "^2.17.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.1.8",
    "jasmine": "^2.5.3",
    "lambda-local": "^1.4.1",
    "proxyquire": "^1.7.11"
  },
  "optionalDependencies": {
    "aws-sdk": "^2.18.0"
  }
}
